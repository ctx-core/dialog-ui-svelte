{
	"name": "@ctx-core/dialog-ui-svelte",
	"version": "1.2.26",
	"description": "ctx-core dialog",
	"keywords": [
		"ctx-core",
		"dialog"
	],
	"homepage": "https://github.com/ctx-core/dialog-ui-svelte#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/dialog-ui-svelte/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/dialog-ui-svelte.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-dialog-ui-svelte@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./CloseDialogHandle"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "uvu . '\\.test\\.(ts|tsx|js|jsx)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"svelte": "^4.2.20"
	},
	"devDependencies": {
		"c8": "^10.1.3",
		"check-dts": "^0.8.2",
		"tsx": "^4.20.6",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}