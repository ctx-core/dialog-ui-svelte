{
	"name": "@ctx-core/dialog-ui-svelte",
	"version": "1.2.3",
	"description": "ctx-core dialog",
	"keywords": [
		"ctx-core",
		"dialog"
	],
	"homepage": "https://github.com/ctx-core/dialog-ui-svelte#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/dialog-ui-svelte/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/dialog-ui-svelte.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./CloseDialogHandle"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "uvu . '\\.test\\.(ts|tsx|js|jsx)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"svelte": "^4.2.3"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.1.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}